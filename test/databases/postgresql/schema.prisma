datasource db {
	provider = "postgresql"
	url      = "postgresql://postgres:pw@localhost:5432/postgres"
}

generator db {
	provider = "go run ../../.."
	output = "./db_gen.go"
	package = "postgresql"
}

model User {
	id       String  @id @default(cuid())
	email    String  @unique
	username String
	name     String?
	stuff    String?
}
