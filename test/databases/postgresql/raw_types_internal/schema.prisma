datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "raw"
}

enum Role {
  user
  admin
}

model User {
  id           String    @id @default(cuid())
  uuid         String?   @db.Uuid
  role         Role
  roleOpt      Role?
  email        String    @unique
  username     String
  str          String
  strOpt       String?
  strEmpty     String?
  time         DateTime
  timeOpt      DateTime?
  timeEmpty    DateTime?
  int          Int
  intOpt       Int?
  intEmpty     Int?
  float        Float
  floatOpt     Float?
  floatEmpty   Float?
  bool         Boolean
  boolOpt      Boolean?
  boolEmpty    Boolean?
  decimal      Decimal
  decimalOpt   Decimal?
  decimalEmpty Decimal?
  json         Json
  jsonOpt      Json?
  jsonEmpty    Json?
  bytes        Bytes
  bytesOpt     Bytes?
  bytesEmpty   Bytes?
}
