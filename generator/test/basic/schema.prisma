datasource db {
	provider = "sqlite"
	url      = "file:dev.sqlite"
}

generator db {
	provider = "../../../prisma-client-go"
	output = "./db_gen.go"
	package = "basic"
}

model User {
	id       String  @id @default(cuid())
	email    String  @unique
	username String
	name     String?
	stuff    String?
}
