datasource db {
	provider = "sqlite"
	url      = "file:dev.sqlite"
}

generator photon {
	provider = "../../../photongo"
	output = "./photon_gen.go"
	package = "types"
}

model User {
	id        String   @id @default(cuid())
	createdAt DateTime @default(now())
	updatedAt DateTime @updatedAt
	str       String
	int       Int
	float     Float
	bool      Boolean
	date      DateTime
}
