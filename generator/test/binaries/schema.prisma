datasource db {
	provider = "sqlite"
	url      = "file:dev.sqlite"
}

generator photon {
	provider = "../../../photongo"
	output = "./photon_gen.go"
	package = "binaries"
	binaryTargets = ["native", "rhel-openssl-1.1.x"]
}

model User {
	id       String  @id @default(cuid())
	email    String  @unique
	username String
	name     String?
	stuff    String?
}
